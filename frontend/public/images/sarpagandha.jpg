<script>
$(document).ready(function() {
  $("body").on("click", ".details-wish", function() {
      var button = $(this);
      var product_id = button.data("wish_product_id");
      var product_name = button.data("wish_product_name");
      var product_price = button.data("wish_product_price");
      var product_image = button.data("wish_image");

      $.ajax({
          url: base_url + "wishlist/add",
          method: "POST",
          data: { product_id, product_name, product_price, product_image },
          success: function(response) {
              if (response.status === 'success') {
                  button.addClass("in-wishlist");
                  toastr.success("Added to wishlist!", "Success");
                  location.reload();
              } else if (response.status === 'exists') {
                  toastr.info("Already in wishlist", "Info");
              }
          }
      });
  });

  
});
</script>